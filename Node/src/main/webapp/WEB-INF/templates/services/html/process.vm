#if (! $user.hasFullViewRights($process))
         <tr>
            <td colspan="2" style="padding-left: 20px;"><span style="color:red; font-weight: bold">The access to the data set is restricted to metadata only. Ask the provider to get full access to the data set.</span></td>
         </tr>
#else
         <tr xmlns:ilcd="http://lca.jrc.it/ILCD">
            <td class="subsection" colspan="2" style="padding-left: 20px;">Inputs</td>
         </tr>
         <tr xmlns:ilcd="http://lca.jrc.it/ILCD">
            <td colspan="2">
               <table width="100%">
                  <tr>
                     <th class="table_header"><a class="info" href="javascript:void(0);">Type Of Flow<span>Names the basic type of the flow.</span></a></th>
